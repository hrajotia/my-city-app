  "scripts": {
    "start": "NODE_ENV=production node app.js",
    "lint": "grunt lint",
    "test": "npm run test:unit",
    "test:unit": "mocha --timeout=30000 ./test/bootstrap.js ./test/*/*.spec.js --exit",
    "cover": "npm run cover:unit && npm run cover:report",
    "cover:unit": "nyc --silent npm run test:unit",
    "cover:report": "nyc report --reporter=lcov --reporter=text",
    "ui:open:src": "babel-node tools/ui/srcServer.js",
    "ui:open:dist": "babel-node tools/ui/distServer.js",
    "ui:start": "concurrently -k -r -s first \"npm run ui:test:watch\" \"npm run ui:open:src\" \"npm run ui:lint:watch\"",
    "ui:build": "babel-node tools/ui/build.js",
    "ui:lint": "esw tools/webpack/webpack.config.* assets/js/webcomponent tools/ui --color --fix",
    "ui:lint:watch": "npm run ui:lint -- --watch",
    "ui:test": "jest -u --coverage",
    "ui:test:watch": "jest -u --coverage --watchAll",
    "ui:analyze-bundle": "babel-node ./tools/ui/analyzeBundle.js"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/hrajotia/sails-react-app.git"
  },
  "author": "hrajotia",
  "license": "MIT",
  "engines": {
    "node": "12",
    "npm": ">=6"
  },
  "jest": {
    "roots": [
      "assets/js/webcomponent",
      "tools"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/tools/ui/assetsTransformer.js",
      "\\.(css|scss)$": "<rootDir>/tools/ui/assetsTransformer.js"
    },
    "setupFiles": [
      "raf/polyfill",
      "./tools/ui/enzymeTestAdapterSetup.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 50,
        "functions": 80,
        "lines": 80
      }
    }
  }
}
